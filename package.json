{
  "name": "ft-uilib",
  "version": "0.0.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "npm-pack": "cd dist/ft-lib && npm pack",
    "test": "ng test",

    "copy-lib":"cp -r dist/ft-lib/* ft-lib-dist",
    "update-lib-version": "cd projects/ft-lib && npm version patch && git push --tags && cd ../../",
    "update-lib-dist": "git add -A && git commit -m=Bambooupdateversion && git push && git subtree push --prefix=ft-lib-dist https://github.com/AbelValdez/ft-lib-dist main",
    "bamboo-build": "npm run update-lib-version && ng build && npm run copy-lib && npm run update-lib-dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.6",
    "@angular/cli": "~13.2.2",
    "@angular/compiler-cli": "~13.2.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.0.0",
    "typescript": "~4.5.2"
  }
}
